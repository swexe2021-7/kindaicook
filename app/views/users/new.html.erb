
<%= form_for @user, url: {action: :create} do |f| %>
  <%= f.label :uid, 'uid' %> 
  <%= f.text_field :uid %> 
  <%= f.label :pass, 'password' %> 
  <%= f.password_field :password %> 
  <%= f.label :pass_confirmation, 'password（確認）' %> 
  <%= f.password_field :pass_confirmation %> 
  <%= f.submit '登録' %>
<% end %>